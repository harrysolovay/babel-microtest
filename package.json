{
  "name": "babel-transform-tester",
  "description": "Framework-agnostic, minimal testing for Babel 7 plugins",
  "version": "0.0.0-development",
  "main": "lib/index.js",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-syntax-jsx": "^7.2.0",
    "@babel/plugin-transform-modules-commonjs": "^7.2.0",
    "@semantic-release/git": "^7.0.8",
    "@semantic-release/npm": "^5.1.4",
    "ava": "^1.2.0",
    "babel-plugin-implicit-function": "^0.1.1",
    "babel-plugin-implicit-return": "^1.0.1",
    "babel-plugin-react-require": "^3.1.1",
    "commitizen": "^3.0.5",
    "cz-conventional-changelog": "^2.1.0",
    "esm": "^3.2.0",
    "semantic-release": "^15.13.3"
  },
  "peerDependencies": {
    "@babel/core": "^7.2.2"
  },
  "ava": {
    "require": [
      "esm"
    ]
  },
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  },
  "release": {},
  "scripts": {
    "test": "ava",
    "clean": "rm -rf lib",
    "build": "babel src --out-dir lib --plugins=@babel/plugin-transform-modules-commonjs",
    "prepublish": "npm run clean && npm run build",
    "semantic-release": "semantic-release",
    "commit": "git-cz"
  },
  "dependencies": {
    "diff": "^4.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/harrysolovay/babel-microtest.git"
  }
}
